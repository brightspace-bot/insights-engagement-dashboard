{
    "env": {
		"es6": true,
		"browser": true
    },
    "extends": [
      "plugin:json/recommended",
      "brightspace/lit-config"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
      "json",
      "html"
    ],
    "rules": {
		"no-console": ["warn"],
		"sort-class-members/sort-class-members": ["warn"]
    }
}
